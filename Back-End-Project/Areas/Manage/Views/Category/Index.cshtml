@model PageNatedList<Category>

@{
    ViewData["Title"] = "Index";
    int count = 0;
}

<div class="row">
    <div class="col-lg-2">
        <h1 class="h3 mb-4 text-gray-800">Categpry Page</h1>
    </div>
    <div class="col-lg-2">
        <a class="btn btn-primary mb-4" asp-action="create" asp-controller="Category" asp-area="manage">Create</a>
    </div>
</div>
<div class="row">
    @await Html.PartialAsync("_CategoryIndexPartial", Model)
</div>
@section Scripts{
    @await Html.PartialAsync("_Validations")
}
